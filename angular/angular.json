{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@crowsong/drawer": {
      "projectType": "library",
      "root": "projects/crowsong/drawer",
      "sourceRoot": "projects/crowsong/drawer/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/crowsong/drawer/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/crowsong/drawer/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/crowsong/drawer/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/crowsong/drawer/src/test.ts",
            "tsConfig": "projects/crowsong/drawer/tsconfig.spec.json",
            "karmaConfig": "projects/crowsong/drawer/karma.conf.js"
          }
        }
      }
    },
    "drawer-test": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/drawer-test",
      "sourceRoot": "projects/drawer-test/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/drawer-test",
            "index": "projects/drawer-test/src/index.html",
            "main": "projects/drawer-test/src/main.ts",
            "polyfills": "projects/drawer-test/src/polyfills.ts",
            "tsConfig": "projects/drawer-test/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/drawer-test/src/favicon.ico",
              "projects/drawer-test/src/assets"
            ],
            "styles": [
              "projects/drawer-test/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/drawer-test/src/environments/environment.ts",
                  "with": "projects/drawer-test/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "drawer-test:build:production"
            },
            "development": {
              "browserTarget": "drawer-test:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "drawer-test:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/drawer-test/src/test.ts",
            "polyfills": "projects/drawer-test/src/polyfills.ts",
            "tsConfig": "projects/drawer-test/tsconfig.spec.json",
            "karmaConfig": "projects/drawer-test/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/drawer-test/src/favicon.ico",
              "projects/drawer-test/src/assets"
            ],
            "styles": [
              "projects/drawer-test/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
